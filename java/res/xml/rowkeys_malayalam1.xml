<merge
    xmlns:latin="http://schemas.android.com/apk/res/in.androidtweak.inputmethod.indic"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
        >
            <!-- ഔ -->
            <Key
                latin:keyLabel="&#x0D14;" />

            <!-- ഐ -->
            <Key
                latin:keyLabel="&#x0D10;" />

            <!-- ആ -->
            <Key
                latin:keyLabel="&#x0D06;" />

            <!-- ഈ -->
            <Key
                latin:keyLabel="&#x0D08;" />

            <!-- ഊ -->
            <Key
                latin:keyLabel="&#x0D0A;" />

            <!-- ഭ -->
            <Key
                latin:keyLabel="&#x0D2D;" />

            <!-- ങ, ങ്ങ-->
            <Key
                latin:keyLabel="&#x0D19;"
                latin:moreKeys="&#x0D19;&#x0D4D;&#x0D19;"
                latin:keyHintLabel="&#x0D19;&#x0D4D;&#x0D19;"
                />

            <!-- ഘ -->
            <Key
                latin:keyLabel="&#x0D18;" />

            <!-- ധ -->
            <Key
                latin:keyLabel="&#x0D27;" />

            <!-- ഝ -->
            <Key
                latin:keyLabel="&#x0D1D;" />
        </case>

        <default>

            <!-- ൗ -->
            <Key
                latin:keyLabel="&#x0D57;" />

            <!-- ൈ -->
            <Key
                latin:keyLabel="&#x0D48;" />

            <!-- ാ -->
            <Key
                latin:keyLabel="&#x0D3E;" />

            <!-- ീ-->
            <Key
                latin:keyLabel="&#x0D40;" />

            <!-- ൂ-->
            <Key
                latin:keyLabel="&#x0D42;" />

            <!-- ബ, ബ്ബ-->
            <Key
                latin:keyLabel="&#x0D2C;"
                latin:moreKeys="&#x0D2C;&#x0D4D;&#x0D2C;"
                latin:keyHintLabel="&#x0D2C;&#x0D4D;&#x0D2C;"
                />

            <!-- ഹ -->
            <Key
                latin:keyLabel="&#x0D39;" />

            <!-- ഗ, ഗ്ഗ -->
            <Key
                latin:keyLabel="&#x0D17;"
                latin:moreKeys="&#x0D17;&#x0D4D;&#x0D17;"
                latin:keyHintLabel="&#x0D17;&#x0D4D;&#x0D17;"
                />

            <!-- ദ, ദ്ദ -->
            <Key
                latin:keyLabel="&#x0D26;"
                latin:moreKeys="&#x0D26;&#x0D4D;&#x0D26;"
                latin:keyHintLabel="&#x0D26;&#x0D4D;&#x0D26;"
                />

            <!-- ജ, ജ്ജ, ₹-->
            <Key
                latin:keyLabel="&#x0D1C;"
                latin:moreKeys="&#x0D1C;&#x0D4D;&#x0D1C;"
                latin:keyHintLabel="&#x0D1C;&#x0D4D;&#x0D1C;"
                latin:additionalMoreKeys="&#x20B9;"
                />
         </default>
    </switch>
</merge>
